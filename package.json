    "xerror": "^1.0.0",
    "zs-objtools": "^1.2.0"
  },
  "devDependencies": {
    "broccoli": "^0.16.7",
    "broccoli-babel-transpiler": "^5.4.3",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^0.2.3",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.2.0",
    "es6-promise": "^3.0.2",
